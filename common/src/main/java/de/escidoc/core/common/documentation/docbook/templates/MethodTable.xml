<table frame="all">
	<title>${TITLE}</title>
	<tgroup cols="4" align="left" colsep="1" rowsep="1">
		<colspec colname="c1" />
		<colspec colname="c2" />
		<colspec colname="c3" />
		<colspec colname="c4" />
		<spanspec spanname="h12span" namest="c1" nameend="c2"
			align="left" />
		<spanspec spanname="h34span" namest="c3" nameend="c4"
			align="left" />
		<spanspec spanname="h234span" namest="c2" nameend="c4"
			align="left" />
		<spanspec spanname="h1234span" namest="c1" nameend="c4"
			align="left" />
		<thead>
			<row>
				<entry spanname="h12span">Http-Request</entry>
				<entry spanname="h34span">
					Invoked Resource Method
				</entry>
			</row>
		</thead>
		<tbody>
			<row>
				<entry spanname="h12span">${HTTP-REQUEST}</entry>
				<entry spanname="h34span">${INVOKED-METHOD}</entry>
			</row>
			<row>
				<entry spanname="h1234span">${DESCRIPTION}</entry>
			</row>
			<row>
				<entry>Replaces</entry>
				<entry spanname="h234span">${OLD-METHOD}</entry>
			</row>
		</tbody>
	</tgroup>
</table>